###############################
# DEV Render Counter Settings #
###############################

# Enable/disable DEV render counter (default true in DEV)
# Accepts: true/false/1/0/on/off
VITE_RENDER_COUNTER_ENABLED=true

# Log level for render counter internal logs (default warn)
# Values: debug | warn | error
VITE_RENDER_COUNTER_LOG_LEVEL=warn

# Existing flag from project: keep as-is, shown here for completeness
VITE_FLOW_STRICT_MODE_ENABLED=false

#########################
# Flow Provider Flags   #
#########################

# Phase 1: Enable FlowProvider by default (dual-wrap with bridge)
VITE_FLOW_PROVIDER_ENABLED=true

# Bridge for AppContext navigation interception (keeps existing behavior)
# Default true to preserve current FlowAppContextBridge semantics
VITE_FLOW_BRIDGE_ENABLED=true
